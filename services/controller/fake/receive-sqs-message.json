{
    "for-react-nginx": {
        "create": {
            "command": "CREATE",
            "createContainerPayload": {
                "id": "random-id",
                "pullImage": "mayursiinh/react-nginx",
                "containerConfig": {
                    "port": 80
                }
            }
        },
        "delete": {
            "command": "REMOVE",
            "removeContainerPayload": {
                "containerName": "random-id"
            }
        }
    }
}